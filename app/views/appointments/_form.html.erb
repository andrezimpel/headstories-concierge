<%= simple_form_for(@appointment) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :start %>
    <%= f.input :end %>
    <%= f.input :notes %>
    <%= f.input :client_id %>
    <%= f.input :alerts %>
    <%= f.input :url %>
    <%= f.input :calendar_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
